{
  "manifest_version": 2,
  "name": "SocialShare",
  "description": "Share anywebsites on click without more hassle",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Share"
  },
  "permissions": [
    "activeTab",
    "tab"
  ]
}